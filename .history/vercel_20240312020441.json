{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": { "distDir": "build" },
      "buildCommands": "next build",
      "framework": "nextjs",
      "installCommand": "pnpm install",
      "env": {
        "NODE_ENV": "production"
      }

    }
  ],
  "routes": [
    { "app": "/(.*)", "dest": "/" }
  ]
}
